{%- assign secSt = section.settings -%}
{%- capture slideShowSec -%}

<div class="content-indent {{ section.settings.top-offeset}}{% if secSt.hide_mobile %} hide-sm{% endif %}{% if secSt.hided %} hide-md hide-lg{% endif %}" data-section-id="{{ section.id }}" data-section-type="slideshow-section">
  {% if section.blocks.size > 0 %}
  <div class="slideshow-wrapper{% if secSt.section_width %} page-width{% else %} fullwidth{% if secSt.npd %} npd{% endif %}{% endif %}{% if section.settings.snow_effect %} falling-snow{% endif %}{% if section.settings.snow_style == "style2" %} style2{% else %} style1{% endif %}">
    {%- capture height -%}
                {% if section.settings.ht == '1' %}
                    {% for block in section.blocks %}
                        {% if block.settings.image != blank %}
                            --slht:{{ 1 | divided_by: block.settings.image.aspect_ratio | times: 100}}%;
                            {% if block.settings.imagem != blank %}
                                --slhtm:{{ 1 | divided_by: block.settings.imagem.aspect_ratio | times: 100}}%;
                            {% else %}
                                --slhtm:{{ 1 | divided_by: block.settings.image.aspect_ratio | times: 100}}%;
                            {% endif %}
                            {% break %}
                        {% else %}
                            --slht:45%;--slhtm:45%;
                        {% endif %}
                    {% endfor %}
                {% else %}
                    --slht:{{section.settings.htd}}px;--slhtt:{{section.settings.htt}}px;--slhtm:{{section.settings.htm}}px;
                {% endif %}
                {% unless secSt.arrow %}--arw:none{% endunless %};	
                {% unless secSt.dot %}--dot:none{% endunless %};	
                --arrow: {{secSt.arrowc}};
	  			 --arrowbg: {{ secSt.arrowbg }};
                 --dotsa: {{ secSt.dots }};
                 --dots: {{secSt.dots | color_modify:'alpha', 0.3 }};
            {%- endcapture -%}
    <div class="slideshow pr ctStyle" style="{{height}}" id="ss{{ section.id }}" data-flickity='{ "cellAlign":"left","wrapAround":true,"fade":true,"selectedAttraction": "0.2","friction":"0.8"{%- if section.settings.autoplay -%},"autoPlay":{{section.settings.speed | times: 1000}}{% endif %}{%- if settings.rtl -%},"rightToLeft": true{%- endif -%} }'>
           
      {% for block in section.blocks %}

                    {%- liquid
                        assign blkSt = block.settings
                        if blkSt.video_url.type == "youtube"
                            assign videotype = "youtube"
                        elsif blkSt.video_url.type == "vimeo"
                            assign videotype = "vimeo"
                        else
                            assign videotype = "video"
                        endif

                        if blkSt.imagem != blank
                            assign dsizes = '1296,1512,2000,2450,3000,3600'
                        else
                            assign dsizes = ''
                        endif    
                        assign msizes = '375,540,780,1080'
        
                        assign imgPos = blkSt.ialign | replace:"_"," "
                        if blkSt.image.presentation.focal_point != blank
                           assign imgPos = blkSt.image.presentation.focal_point
                        endif
                        assign img2Pos = blkSt.ialign | replace:"_"," "
                        if blkSt.imagem != blank and blkSt.imagem.presentation.focal_point != blank
                           assign img2Pos = blkSt.imagem.presentation.focal_point
                        endif
                    %}
                    <div class="sslide of_hidden{% if section.settings.ht == '1' %} im{% else %} ct{% endif %} sl{{ block.id }}" data-type="{{block.type}}" {% if block.type == 'video' %} data-video="{{videotype}}"{% endif %} {{ block.shopify_attributes }} style="--brd:{{section.settings.brd}}px;">
                        {% if blkSt.display_btn != true and blkSt.link != blank and block.type != 'video' %}<a href="{{ blkSt.link }}" class="ss_link" {% if blkSt.btn1window %} target="_blank" {% endif %}>{% endif %}
                        {% if block.type == 'image' and blkSt.image != blank %}
                            <picture class="slide-image">
                                {% if blkSt.imagem != blank %}
                                    <source media="(max-width:767px)" srcset="{{ blkSt.imagem | img_url: '700x' }}">
                                {% endif %}
                                <source media="(max-width:1280px)" srcset="{{ blkSt.image | img_url: '1400x' }}">
                                <img class="ssimg imgFt{% if section.settings.imganim %} anim{% endif %}" src="{{ blkSt.image | img_url: '2000x' }}" width="{{ blkSt.image.width }}" height="{{ blkSt.image.height }}" alt="{{ blkSt.image.alt | escape }}"
                                  {% if section.index < 2 and forloop.index == 1 %}fetchpriority="high"{% else %}loading="lazy"{% endif %} style="--ipos:{{ imgPos }};--iposm:{{ img2Pos }}">
                            </picture>
                        {% elsif block.type == 'video' and blkSt.video_url != blank %}
                            {% if blkSt.video_url.type == "youtube" %}
                                <iframe class="embed-player slide-media" width="980" height="520" src="https://www.youtube.com/embed/{{ blkSt.video_url.id }}?enablejsapi=1&controls=0&fs=0&iv_load_policy=3&rel=0&showinfo=0&loop=1&playlist={{blkSt.video_url.id}}&playsinline=1&autoplay=1&mute=1" allow="autoplay" frameborder="0" webkitallowfullscreen mozallowfullscreen allowfullscreen></iframe>
                            {% elsif blkSt.video_url.type == "vimeo" %}
                                <iframe class="embed-player slide-media" src="https://player.vimeo.com/video/{{blkSt.video_url.id}}?api=1&byline=0&portrait=0&title=0&background=1&mute=1&loop=1&autoplay=0&id={{blkSt.video_url.id}}" width="980" height="520" frameborder="0" webkitallowfullscreen mozallowfullscreen allowfullscreen></iframe>
                            {% endif %}
                        {% elsif block.type == 'video' and blkSt.video != blank %}
                            {{ blkSt.video | video_tag :loop:true, autoplay:true, muted:true, playsinline:true, class:'slide-video slide-media' }} 
                        {% else %}
                            <div class="ssimg h_100 js">
                                <div class="ph_bg h_100">{% capture current %}{% cycle 1, 2 %}{% endcapture %}{{ 'lifestyle-' | append: current | placeholder_svg_tag: 'placeholder-svg' }}</div>
                            </div>
                        {% endif %}
                        {%- capture bxStyle -%}
                            {% if secSt.isbox %}
                                --bg:{{ secSt.tbg | color_modify: 'alpha', secSt.opt}};--pd:40px;
                            {% else %}
                                --bg:transparent;--pd:0;--frm:none;
                            {% endif %}
                            {% if secSt.isboxm %}
                                --bgm:{{ secSt.tbg | color_modify: 'alpha', secSt.opt}};--pdm:20px;
                            {% else %}
                                --bgm:transparent;--pdm:0;--frcl:transparent;--frmm:none;
                            {% endif %}
                            {% if secSt.isbox or secSt.isboxm %}
                                --brad:{{secSt.brad}}px;
                            {% endif %}
                        {%- endcapture -%}
                        <div class="ss_txt-wrap w_100 h_100" {% if secSt.isovr %}style="--ibg:{{ secSt.obg | color_modify: 'alpha', secSt.oopt }};"{% endif %}>
                            <div class="page-width fl h_100 {{ blkSt.txtpos }} {{ blkSt.txtposm }}" style="max-width:{{blkSt.contwd}}px;{{bxStyle}}">
                                <div class="ss_txt{% if blkSt.text-left %} tl{% else %} tc{% endif %}{% if section.settings.txtanimation %} anim {{ section.settings.anim }}{% endif %}" style="--textwid:{{  blkSt.text_width }}px;">
                                    {% if blkSt.hd1 != blank %}
                                        <div class="stxt hd1 {{secSt.pff}}" style="--cl:{{blkSt.pcl}};--phbg:{{blkSt.phbg}};--fs:{{secSt.pfs}}px;--fsm:{{secSt.pfsm}}px;--fw:{{secSt.pfw}};--lh:{{secSt.plh}};--mb:{{secSt.pmb}}px;--mbm:{{secSt.pmbm}}px"><p class="dib">{{ blkSt.hd1 }}</p></div>
                                    {% endif %}
                                    {% if blkSt.title != blank %}
                                        <h2 class="stxt hd2 {{secSt.bff}}" style="--cl:{{blkSt.tcl}};--fs:{{secSt.bfs}}px;--fsm:{{secSt.bfsm}}px;--fw:{{secSt.bfw}};--lh:{{secSt.blh}};--mb:{{secSt.bmb}}px;--mbm:{{secSt.bmbm}}px">{{blkSt.title}}</h2>
                                    {% endif %}
                                    {% if blkSt.hd3 != blank %}
                                        <div class="stxt hd3 {{secSt.dff}}{% if blkSt.hideh3 %} hide-sm{% endif %}" style="--cl:{{blkSt.cl}};--fs:{{secSt.dfs}}px;--fsm:{{secSt.dfsm}}px;--fw:{{secSt.dfw}};--lh:{{secSt.dlh}};--mb:{{secSt.dmb}}px;--mbm:{{secSt.dmbm}}px">{{ blkSt.hd3 }}</div>
                                    {% endif %}
                                    {% if blkSt.display_btn %}
                                        <div class="btnWrap">
                                            <a class="btn {{secSt.btn-sizem}} {{secSt.btn-size}} {{blkSt.btn-style}}" href="{{ blkSt.link }}" {% if blkSt.btn1window %} target="_blank" {% endif %} >{{ blkSt.btn }}</a>
                                            <a class="btn {{secSt.btn-sizem}} {{secSt.btn-size}} {{blkSt.btn-style1}}" href="{{ blkSt.link2 }}" {% if blkSt.btn2window %} target="_blank" {% endif %}>{{ blkSt.btn2 }}</a>
                                        </div>
                                    {% elsif block.type == 'video' %}
                                        {% if blkSt.btn != blank %}<div class="btnWrap"><a class="btn {{secSt.btn-sizem}} {{secSt.btn-size}} {{blkSt.btn-style}}" href="{{ blkSt.link }}">{{blkSt.btn}}</a></div>{% endif %}
                                    {% else %}
                                        {% if blkSt.btn != blank %}<div class="btnWrap"><span class="btn {{secSt.btn-sizem}} {{secSt.btn-size}} {{blkSt.btn-style}}" >{{blkSt.btn}}</span></div>{% endif %}
                                    {% endif %}            
                                </div>
                            </div>
                        </div>             
                        {% if blkSt.display_btn != true and blkSt.link != blank and block.type != 'video' %}</a>{% endif %}
                    </div>
                {% endfor %}
    </div>
  </div>
  {% endif %}
  {% if section.blocks.size == 0 %}<div class="placeholder-noblocks">{{ 'homepage.onboarding.no_content' | t }}</div>{% endif %}
</div>
{%- endcapture -%}
{{slideShowSec | strip_newlines | remove: "  " | remove: "	" }}
