{%- assign secSt = section.settings -%}
{%- capture gridBanners -%}
<div class="content-indent {{ secSt.top-offeset}}{% if secSt.hide_mobile %} hide-sm{% endif %}{% if secSt.hided %} hide-md hide-lg{% endif %}"> {% capture imageGallery %}
  {% capture styles %}
  --cl:{{secSt.cl}};--fs:{{secSt.fs}}px;--fsm:{{secSt.fsm}}px;--fw:{{secSt.fw}};--lh:{{secSt.lh}};
  --tcl:{{secSt.cl2}};--tfs:{{secSt.tfs}}px;--tfsm:{{secSt.tfsm}}px;--tfw:{{secSt.tfw}};--tlh:{{secSt.tlh}};--tff:var(--{{ secSt.tff }});
  {% if secSt.isbox %}
  --bg:{{secSt.bg | color_modify:'alpha',secSt.opt}};--bdr:{{secSt.bdr}}px;
  {% endif %}
  {% endcapture %}
  <div class="imgBnrOuter sec-{{section.id}}">
    <div class="{% if secSt.fullwidth %}fullwidth{% if secSt.npd %} npd{% endif %}{% else %}page-width{%endif%}"style="{{styles}}"> {% render 'sec-title' %}
      
      {% if secSt.snow_effect %}
      <div class="falling-snow{% if secSt.snow_style == "style2" %} style2{% else %} style1{% endif %}">
      {% endif %}
      
      
      {%- for block in section.blocks -%}
      {%- assign blkSt = block.settings -%}
      {%- if block.type == 'style1' -%}
      <div class="grid-bnr st1{% unless secSt.disable_zoom %} zoom_hov{% endunless %}" {{ block.shopify_attributes }}>
        <div class="imgBnr wow fadeInUp" data-wow-delay="0ms"> {% render 'image-grid',image:blkSt.image1, title:blkSt.text1, subtitle:blkSt.text1-large, link:blkSt.link1, talign:blkSt.bnr1align, isBox:secSt.isbox, imgSize:'930x620' %} </div>
