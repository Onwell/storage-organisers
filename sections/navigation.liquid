{%- if section.blocks.size > 0 -%}
{%- assign secSt = section.settings -%}
<ul id="siteNav" class="siteNavigation {{ section.settings.menuAlign}}{% if settings.nav_below_logo %} fwmm{% unless settings.main_header_fullwidth %} page-width{% endunless %}{% endif %}" data-section-id="site-{{ section.id }}" data-section-type="site-navigation">
{% for block in section.blocks %}
    {%- assign blkSt = block.settings -%}
    {%- assign label = blkSt.title | split: '[label' | last  | split: ']' | first | prepend: "nm_label" -%}
    {%- capture linkLevel1 -%}<a href="{% if blkSt.url != blank %}{{ blkSt.url }}{% else %}#{% endif %}" {% if blkSt.newtab %} target="_blank"{% endif %}>{{ blkSt.title | split: '[label' | first }}{% if section.settings.menuarrow %} <i class="at at-angle-down-l"></i>{% endif %}{% if blkSt.title contains '[label' %}<span class="lbl {{label}}">{{ section.settings[label] }}</span>{% endif %}</a>{%- endcapture -%}
    {%- case block.type -%}
    {%- when 'homelink' -%}
    <li class="lvl1"><a href="{{routes.root_url}}">{{ blkSt.title}}</a></li>

    {%- when 'loginlink' -%}
    {% if customer %}
    <li class="lvl1"><a href="{{ routes.account_url }}">{{ 'customer.account.title' | t }}</a></li>
    <li class="lvl1"><a href="{{ routes.account_logout_url }}">{{ 'customer.account.logout' | t }}</a></li>    
    {% else %}
    <li class="lvl1"><a href="{{ routes.account_login_url }}">{{ 'customer.login.form_title' | t }}</a></li> 
    {% endif %}	  

    {%- when 'single_link' -%}
    <li class="lvl1" {{ block.shopify_attributes }}>{{linkLevel1 | replace: '<i class="at at-angle-down-l"></i>','' }}</li>

    {%- when 'megamenu1' -%}
    {%- if blkSt.title != blank -%}
    <li class="lvl1 parent megamenu{% if blkSt.custom_size %} ctsize{% endif %}" {{ block.shopify_attributes }} style="--img_rad:{{blkSt.img_rad}}px" >{{linkLevel1}}
      <div class="megamenu style1{% if blkSt.imgcall == "right" %} flex f-ais{% endif %}{% if blkSt.custom_size %} {{blkSt.mm_align}}{% else %} mmfull{% endif %}"{% if blkSt.custom_size %}style="width:{{blkSt.mm_size}}px;"{% endif %}>
          {% if blkSt.box_sized %}<div class="mm_layout{% if blkSt.imgcall == "right" %} fl f-ais{% endif %}">{% endif %}
        	{%- capture imgColumns -%}
            {%- capture my_variable -%}{{blkSt.megamenu1_image}} {{blkSt.megamenu2_image}} {{blkSt.megamenu3_image}} {{blkSt.megamenu4_image}}{%- endcapture -%}
            {%- assign products_array = my_variable | split: ' ' -%}
            {%- if products_array.size > 0 -%}
                {%- if blkSt.megamenu1_image != blank -%}
                	<li class="gitem imageCol tc">
                      <a href="{{ blkSt.imageurl }}">
                        <img class="autoHt lazyload" data-src="{{ blkSt.megamenu1_image | img_url: 'master' }}" width="{{ blkSt.megamenu1_image.width }}" height="{{ blkSt.megamenu1_image.height }}" alt="{{blkSt.megamenu1_image.alt}}">
                        {% if blkSt.imageTtl1 != blank %}<span class="ttl{% if blkSt.show_bdr %} bdr{% endif %}">{{blkSt.imageTtl1}}</span>{% endif %}
                      </a>
                	</li>
                {%- endif -%}
                {%- if blkSt.megamenu2_image != blank -%}
                	<li class="gitem imageCol tc">
                      <a href="{{ blkSt.Image2url }}">
                        <img class="autoHt lazyload" data-src="{{ blkSt.megamenu2_image | img_url: 'master' }}" width="{{ blkSt.megamenu2_image.width }}" height="{{ blkSt.megamenu2_image.height }}" alt="{{blkSt.megamenu2_image.alt}}">
                      	{% if blkSt.imageTtl2 != blank %}<span class="ttl{% if blkSt.show_bdr %} bdr{% endif %}">{{blkSt.imageTtl2}}</span>{% endif %}
                      </a>
                	</li>
                {%- endif -%}
                {%- if blkSt.megamenu3_image != blank -%}
                	<li class="gitem imageCol tc">
                      <a href="{{ blkSt.Image3url }}">
                        <img class="autoHt lazyload" data-src="{{ blkSt.megamenu3_image | img_url: 'master' }}" width="{{ blkSt.megamenu3_image.width }}" height="{{ blkSt.megamenu3_image.height }}" alt="{{blkSt.megamenu3_image.alt}}">
                      	{% if blkSt.imageTtl3 != blank %}<span class="ttl{% if blkSt.show_bdr %} bdr{% endif %}">{{blkSt.imageTtl3}}</span>{% endif %}
                      </a>
                	</li>
                {%- endif -%}
                {%- if blkSt.megamenu4_image != blank -%}
                	<li class="gitem imageCol tc">
                      <a href="{{ blkSt.Image4url }}">
                        <img class="autoHt lazyload" data-src="{{ blkSt.megamenu4_image | img_url: 'master' }}" width="{{ blkSt.megamenu4_image.width }}" height="{{ blkSt.megamenu4_image.height }}" alt="{{blkSt.megamenu4_image.alt}}">
                        {% if blkSt.imageTtl4 != blank %}<span class="ttl{% if blkSt.show_bdr %} bdr{% endif %}">{{blkSt.imageTtl4}}</span>{% endif %}
                      </a>
                	</li>
                {%- endif -%}
          	{%- endif -%}
          {%- endcapture -%}
          {% if blkSt.megamenu1_image != blank or blkSt.megamenu2_image != blank or blkSt.megamenu3_image != blank or blkSt.megamenu4_image != blank %}
       		<ul class="grid rwcols-{{blkSt.mmcolumns}}{% if blkSt.imgcall == "right" %} flex-item{%- endif -%}">{% for link in linklists[blkSt.mm_linklist].links %}{% render 'mm-links', link:link %}{% endfor %}</ul>
           	<ul class="grid f-ais rwcols-{{blkSt.mmimgcolumns}}" {% if blkSt.imgcall == "right" %}style="width:{{blkSt.columnWidth}}%;margin-left:15px;"{%- endif -%}>{{imgColumns}}</ul>
          {%- else -%}
        	<ul class="grid rwcols-{{blkSt.mmcolumns}} flex-item mmWrapper">{% for link in linklists[blkSt.mm_linklist].links %}{% render 'mm-links', link:link %}{% endfor %}</ul>
          {%- endif -%}
         {% if blkSt.box_sized %}</div>{% endif %}
      </div>
    </li>
    {%- endif -%}

    {%- when 'megamenu2' -%}
    {%- if blkSt.title != blank -%}
    <li class="lvl1 parent megamenu{% if blkSt.custom_size %} ctsize{% endif %}" style="--img_rad:{{blkSt.img_rad}}px" {{ block.shopify_attributes }}>{{linkLevel1}}
      <div class="megamenu grid--table style2{% if blkSt.custom_size %} {{blkSt.mm_align}}{% else %} mmfull{% endif %}"{% if blkSt.custom_size %}style="width:{{blkSt.mm_size}}px;"{% endif %}>
       {% if blkSt.box_sized %}<div class=" grid--table mm_layout">{% endif %}
        <div class="gitem prColumn">
          <ul class="grid rwcols-{{blkSt.mmcolumns}}">
            {%- assign collection = collections[blkSt.collection] -%}
            {% for product in collection.products limit: blkSt.pr_limit %}
            <li class="gitem productCol">{% render 'mm-product', product:product %}</li>
            {% endfor %}
          </ul>
          {%- if blkSt.prodall != blank -%}<a href="{{collection.url}}" class="btn">{{blkSt.prodall}}</a>{%- endif -%}
        </div>
        {%- if blkSt.ftcl -%}
        <div class="gitem mmftcolumn tc" style="background-color:{{blkSt.ftcl_bg}};width:{{blkSt.ftcl_width}}%;">
          {%- if blkSt.ft_link != blank -%}<a href="{{blkSt.ft_link}}">{%- endif -%}
          {%- if blkSt.ftcl_image != blank -%}
          <p><img class="imgFl lazyload" src="{{ blkSt.ftcl_image | img_url: '50x' }}" width="{{ blkSt.ftcl_image.width }}" height="{{ blkSt.ftcl_image.height }}"
               data-srcset="{% render 'lazyload', image: blkSt.ftcl_image, sizes: '180,360,540,720,900' %}" data-sizes="auto" alt="{{ blkSt.ftcl_image.alt | escape }}"></p>
          {%- endif -%}
          {%- if blkSt.ftcl_title != blank -%}<h4 style="color:{{blkSt.ftcl_txt}};font-size:{{blkSt.ftcl_title_size}}px;">{{blkSt.ftcl_title}}</h4>{%- endif -%}
          {%- if blkSt.ftcl_title != blank -%}<p style="color:{{blkSt.ftcl_txt}};font-size:{{blkSt.ftcl_subtitle_size}}px;">{{blkSt.ftcl_subtitle}}</p>{%- endif -%}
          {%- if blkSt.ftcl_title != blank -%}<span class="btn" style="color:{{blkSt.ftcl_txt}};background-color:{{blkSt.ftcl_bg}}; border:2px solid {{blkSt.ftcl_txt}};">{{blkSt.ftcl_btn}}</span>{%- endif -%}
          {%- if blkSt.ft_link != blank -%}</a>{%- endif -%}
        </div>
         {% if blkSt.box_sized %}</div>{% endif %}
      </div>
      {%- endif %}
    </li>
    {%- endif -%}

    {%- when 'megamenu3' -%}
    {%- if blkSt.title != blank -%}
    <li class="lvl1 parent megamenu{% if blkSt.custom_size %} ctsize{% endif %}" {{ block.shopify_attributes }}>{{linkLevel1}}
      <div class="megamenu style3{% if blkSt.custom_size %} {{blkSt.mm_align}}{% else %} mmfull{% endif %}"{% if blkSt.custom_size %}style="width:{{blkSt.mm_size}}px;"{% endif %}>
       {% if blkSt.box_sized %}<div class="mm_layout">{% endif %}
          {%- capture menucolumns -%}{{blkSt.ttl1 | remove: " "}} {{blkSt.ttl2 | remove: " "}} {{blkSt.ttl3 | remove: " "}} {{blkSt.ttl4 | remove: " "}} {{blkSt.ttl5 | remove: " "}}{%- endcapture -%}
          {%- assign columnitems = menucolumns | split: ' ' -%}
        <ul class="grid rwcols-{{columnitems.size}} mmWrapper" style="--img_rad:{{blkSt.img_rad}}px">
          {%- assign mm3img = '380x' -%}
          {%- if columnitems.size == 4 -%}
              {% assign mm3img = '475x' %}
          {% elsif columnitems.size == 3 %}
              {% assign mm3img = '640x' %}
          {% elsif columnitems.size == 2 %}
              {% assign mm3img = '900x' %}
          {% elsif columnitems.size == 1 %}
              {% assign mm3img = '1900x' %}
          {%- endif -%}

          {%- if blkSt.ttl1 != blank -%}
          <li class="gitem lvl-1">
            {%- assign m3label1 = blkSt.ttl1 |  split: '[label' | last | split: ']' | first | prepend: "nm_label" -%}
            <a href="{% if blkSt.ttlurl1 != blank %}{{ blkSt.ttlurl1 }}{% else %}#{% endif %}" class="lvl-1">
          	  <img class="autoHt lazyload" width="{{ blkSt.megamenu1_image.width }}" height="{{ blkSt.megamenu1_image.height }}"
                   data-srcset="{% render 'lazyload', image: blkSt.megamenu1_image, sizes: '180,360,540,720' %}" data-sizes="auto" alt="{{ blkSt.megamenu1_image.alt | escape }}">
              <span class="title">{{ blkSt.ttl1 | split: '[label' | first }} {% if blkSt.ttl1 contains '[label' %}<span class="lbl {{m3label1}}">{{ section.settings[m3label1] }}</span>{% endif %}</span>
            </a>
            <ul>
              {% for link in linklists[blkSt.mm_linkcol1].links %}
              	<li class="lvl-2{% if link.active %} active{% endif %}">
                  {%- assign label = link.title |  split: '[label' | last | split: ']' | first | prepend: "nm_label" -%}
                  <a href="{{ link.url }}" class="site-nav">{{ link.title | split: '[label' | first }}{% if link.title contains '[label' %}<span class="lbl {{label1}}">{{ section.settings[label] }}</span>{% endif %}</a>
              	</li>
              {% endfor %}
            </ul>
          </li>
          {%- endif -%}
          {%- if blkSt.ttl2 != blank -%}
          <li class="gitem lvl-1">
            {%- assign m3label2 = blkSt.ttl2 |  split: '[label' | last | split: ']' | first | prepend: "nm_label" -%}
            <a href="{% if blkSt.ttlurl2 != blank %}{{ blkSt.ttlurl2 }}{% else %}#{% endif %}" class="lvl-1">
          	  <img class="autoHt lazyload" width="{{ blkSt.megamenu2_image.width }}" height="{{ blkSt.megamenu2_image.height }}" 
                   data-srcset="{% render 'lazyload', image: blkSt.megamenu2_image, sizes: '180,360,540,720' %}" data-sizes="auto" alt="{{ blkSt.megamenu2_image.alt | escape }}">
              <span class="title">{{ blkSt.ttl2 | split: '[label' | first }} {% if blkSt.ttl2 contains '[label' %}<span class="lbl {{m3label2}}">{{ section.settings[m3label2] }}</span>{% endif %}</span>
            </a>
            <ul>
              {% for link in linklists[blkSt.mm_linkcol2].links %}
              	<li class="lvl-2{% if link.active %} active{% endif %}">
                  {%- assign label = link.title |  split: '[label' | last | split: ']' | first | prepend: "nm_label" -%}
                  <a href="{{ link.url }}" class="site-nav">{{ link.title | split: '[label' | first }}{% if link.title contains '[label' %}<span class="lbl {{label1}}">{{ section.settings[label] }}</span>{% endif %}</a>
              	</li>
              {% endfor %}
            </ul>
          </li>
          {%- endif -%}
          {%- if blkSt.ttl3 != blank -%}
          <li class="gitem lvl-1">
            {%- assign m3label3 = blkSt.ttl3 |  split: '[label' | last | split: ']' | first | prepend: "nm_label" -%}
            <a href="{% if blkSt.ttlurl3 != blank %}{{ blkSt.ttlurl3 }}{% else %}#{% endif %}" class="lvl-1">
          	  <img class="autoHt lazyload" width="{{ blkSt.megamenu3_image.width }}" height="{{ blkSt.megamenu3_image.height }}"
                  data-srcset="{% render 'lazyload', image: blkSt.megamenu3_image, sizes: '180,360,540,720' %}" data-sizes="auto" alt="{{ blkSt.megamenu3_image.alt | escape }}">
              <span class="title">{{ blkSt.ttl3 | split: '[label' | first }} {% if blkSt.ttl3 contains '[label' %}<span class="lbl {{m3label3}}">{{ section.settings[m3label3] }}</span>{% endif %}</span>
            </a>
            <ul>
              {% for link in linklists[blkSt.mm_linkcol3].links %}
              	<li class="lvl-2{% if link.active %} active{% endif %}">
                  {%- assign label = link.title |  split: '[label' | last | split: ']' | first | prepend: "nm_label" -%}
                  <a href="{{ link.url }}" class="site-nav">{{ link.title | split: '[label' | first }}{% if link.title contains '[label' %}<span class="lbl {{label1}}">{{ section.settings[label] }}</span>{% endif %}</a>
              	</li>
              {% endfor %}
            </ul>
          </li>
          {%- endif -%}
          {%- if blkSt.ttl4 != blank -%}
          <li class="gitem lvl-1">
            {%- assign m3label4 = blkSt.ttl4 |  split: '[label' | last | split: ']' | first | prepend: "nm_label" -%}
            <a href="{% if blkSt.ttlurl4 != blank %}{{ blkSt.ttlurl4 }}{% else %}#{% endif %}" class="lvl-1">
          	  <img class="autoHt lazyload" width="{{ blkSt.megamenu4_image.width }}" height="{{ blkSt.megamenu4_image.height }}"
                  data-srcset="{% render 'lazyload', image: blkSt.megamenu4_image, sizes: '180,360,540,720' %}" data-sizes="auto" alt="{{ blkSt.megamenu4_image.alt | escape }}">
              <span class="title">{{ blkSt.ttl4 | split: '[label' | first }} {% if blkSt.ttl4 contains '[label' %}<span class="lbl {{m3label4}}">{{ section.settings[m3label4] }}</span>{% endif %}</span>
            </a>
            <ul>
              {% for link in linklists[blkSt.mm_linkcol4].links %}
              	<li class="lvl-2{% if link.active %} active{% endif %}">
                  {%- assign label = link.title |  split: '[label' | last | split: ']' | first | prepend: "nm_label" -%}
                  <a href="{{ link.url }}" class="site-nav">{{ link.title | split: '[label' | first }}{% if link.title contains '[label' %}<span class="lbl {{label1}}">{{ section.settings[label] }}</span>{% endif %}</a>
              	</li>
              {% endfor %}
            </ul>
          </li>
          {%- endif -%}
          {%- if blkSt.ttl5 != blank -%}
          <li class="gitem lvl-1">
            {%- assign m3label5 = blkSt.ttl5 |  split: '[label' | last  | split: ']' | first | prepend: "nm_label" -%}
            <a href="{% if blkSt.ttlurl5 != blank %}{{ blkSt.ttlurl5 }}{% else %}#{% endif %}" class="lvl-1">
          	  <img class="autoHt lazyload" width="{{ blkSt.megamenu5_image.width }}" height="{{ blkSt.megamenu5_image.height }}"
                  data-srcset="{% render 'lazyload', image: blkSt.megamenu5_image, sizes: '180,360,540,720' %}" data-sizes="auto" alt="{{ blkSt.megamenu5_image.alt | escape }}">
              <span class="title">{{ blkSt.ttl5 | split: '[label' | first }} {% if blkSt.ttl5 contains '[label' %}<span class="lbl {{m3label5}}">{{ section.settings[m3label5] }}</span>{% endif %}</span>
            </a>
            <ul>
              {% for link in linklists[blkSt.mm_linkcol5].links %}
              	<li class="lvl-2{% if link.active %} active{% endif %}">
                  {%- assign label = link.title |  split: '[label' | last | split: ']' | first | prepend: "nm_label" -%}
                  <a href="{{ link.url }}" class="site-nav">{{ link.title | split: '[label' | first }}{% if link.title contains '[label' %}<span class="lbl {{label1}}">{{ section.settings[label] }}</span>{% endif %}</a>
              	</li>
              {% endfor %}
            </ul>
          </li>
          {%- endif -%}
        </ul>
         {% if blkSt.box_sized %}</div>{% endif %}
      </div>
    </li>
    {%- endif -%}

    {%- when 'megamenu4' -%}
    {%- if blkSt.title != blank -%}
    <li class="lvl1 parent megamenu{% if blkSt.custom_size %} ctsize{% endif %}" {{ block.shopify_attributes }}>{{linkLevel1}}
      {%- if linklists[blkSt.mm_linklist].links != blank -%}
      <div class="megamenu style4{% if blkSt.custom_size %} {{blkSt.mm_align}}{% else %} mmfull{% endif %}" style="{% if blkSt.custom_size %}width:{{blkSt.mm_size}}px;{% endif %}{% if blkSt.mm4_bg != blank %}background-image:url({{blkSt.mm4_bg | img_url: "master"}});background-position:top {{blkSt.mm4_img_position}};{% endif %}">
         {% if blkSt.box_sized %}<div class="mm_layout">{% endif %}
          <ul class="grid rwcols-{{blkSt.mmcolumns}} mmWrapper" {% if blkSt.mm4_bg != blank %}style="padding-{{blkSt.mm4_img_position}}:{{blkSt.mm4_space}};"{% endif %}>
            {% for link in linklists[blkSt.mm_linklist].links %}{%- render 'mm-links', link:link -%}{% endfor %}
          </ul>
        {% if blkSt.box_sized %}</div>{% endif %}
      </div>
      {%- endif -%}
    </li>
    {%- endif -%}

    {%- when 'megamenu5' -%}
    {%- if blkSt.title != blank -%}
    <li class="lvl1 parent megamenu{% if blkSt.custom_size %} ctsize{% endif %}" {{ block.shopify_attributes }}>{{linkLevel1}}
      <div class="megamenu style5{% if blkSt.custom_size %} {{blkSt.mm_align}}{% else %} mmfull{% endif %}"{% if blkSt.custom_size %} style="width:{{blkSt.mm_size}}px;"{% endif %}>
        {% if blkSt.box_sized %}<div class="mm_layout">{% endif %}
        <ul class="grid rwcols-{{blkSt.mmcolumns}} mmWrapper {{blkSt.mm_linkStyle}}" style="--img_rad:{{blkSt.img_rad}}px" >
          {%- for link in linklists[blkSt.mm_linklist].links -%}
          <li class="gitem lvl-1">
            {%- assign navImg = link.title | handleize -%}
            <a href="{{ link.url }}" class="site-nav lvl-1">
              {%- if blkSt.mm_linkStyle == "above" -%}<span class="imgOver">{{ link.title }}</span>{%- endif -%}
              <img class="imgFl lazyload" data-src="{{ navImg | append: '.jpg' | file_img_url: '400x' }}" width="400" height="400" alt="{{ link.title }}" />
              {%- if blkSt.mm_linkStyle != "above" -%}<span class="imgOver">{{ link.title }}</span>{%- endif -%}
            </a>
          </li>
          {%- endfor -%}
        </ul>
        {% if blkSt.box_sized %}</div>{% endif %}
      </div>
    </li>
    {%- endif -%}

    {%- when 'megamenu7' -%}
    {%- if blkSt.title != blank -%}
    <li class="lvl1 parent megamenu{% if blkSt.custom_size %} ctsize{% endif %}" {{ block.shopify_attributes }}>{{linkLevel1}}
      <div class="megamenu style5{% if blkSt.custom_size %} {{blkSt.mm_align}}{% else %} mmfull{% endif %}"{% if blkSt.custom_size %} style="width:{{blkSt.mm_size}}px;"{% endif %}>
        {% if blkSt.box_sized %}<div class="mm_layout">{% endif %}
        {{ blkSt.mm_html }}
        <div>&nbsp;</div>
        {% if blkSt.box_sized %}</div>{% endif %}
      </div>
    </li>
    {%- endif -%}
  
  	{%- when 'megamenu6' -%}
    {%- if blkSt.title != blank -%}
    <li class="lvl1 parent megamenu{% if blkSt.custom_size %} ctsize{% endif %}" {{ block.shopify_attributes }}>{{linkLevel1}}
      <div class="megamenu style6{% if blkSt.imgcall == "right" %} flex{% endif %}{% if blkSt.custom_size %} {{blkSt.mm_align}}{% else %} mmfull{% endif %}"{% if blkSt.custom_size %}style="width:{{blkSt.mm_size}}px;"{% endif %}>
          {% if blkSt.box_sized %}<div class="mm_layout{% if blkSt.imgcall == "right" %} flex{% endif %}">{% endif %}
        	<ul class="fl w_100 mmWrapper">
              <li class="mm6main">
                  {% for link in linklists[blkSt.mm_linklist].links %}
                      {%- assign label1 = link.title |  split: '[label' | last | split: ']' | first | prepend: "nm_label" -%}
                      <a href="{{ link.url }}" class="site-nav lvl-1{% if link.links != blank %} hasSub{% endif %}{% if forloop.first %} active{% endif %}" data-link="#nv{{link.title | handleize}}">{{ link.title | split: '[label' | first }}{% if link.title contains '[label' %}<span class="lbl {{label1}}">{{ section.settings[label1] }}</span>{% endif %}</a>
                  {% endfor %}
              </li>
        	  <li class="lvl-1 fl1">
                  {% for link in linklists[blkSt.mm_linklist].links %}
                	{% assign nindex = forloop.index %}
                	<div id="nv{{link.title | handleize}}" class="fl admmsub{% if forloop.first %} active{% endif %}">
                      {%- if link.links != blank -%}
                        <ul class="subLinks fl1">
                          <li class="fl1 lvl-2{% if childlink.active or childlink.child_active %} active{% endif %}" style="column-count:{{blkSt.mmcolumns}};">
                          {%- for childlink in link.links -%}
                            {%- assign label2 = childlink.title |  split: '[label' | last  | split: ']' | first | prepend: "nm_label" -%}
                            <a href="{{ childlink.url }}" class="site-nav lvl-2">{{ childlink.title | split: '[label' | first }}{% if childlink.title contains '[label' %}<span class="lbl {{label2}}">{{ section.settings[label2] }}</span>{% endif %}</a>
                            {%- if childlink.links != blank -%}
                            <ul class="subLinks">
                              {%- for lvl3link in childlink.links -%}
                              <li class="lvl-3 {% if lvl3link.active %}active{% endif %}">
                                {%- assign label3 = lvl3link.title |  split: '[label' | last  | split: ']' | first | prepend: "nm_label" -%}
                                <a href="{{ lvl3link.url }}" class="site-nav lvl-3">{{ lvl3link.title | split: '[label' | first }}{% if lvl3link.title contains '[label' %}<span class="lbl {{label3}}">{{ section.settings[label3] }}</span>{% endif %}</a>
                              </li>
                              {%- endfor -%}
                            </ul>
                            {%- endif -%}
